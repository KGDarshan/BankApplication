package com.darshan.controller;

import java.io.IOException;
import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;


public class Register extends HttpServlet {
	
	protected void service(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		
		String name = request.getParamter("name");
		String custid = request.getParamter("custid");
		String accno = request.getParamter("accno");
		String password = request.getParamter("pwd");
		String balance = request.getParamter("bal");
		String email = request.getParamter("email");
	}

}
